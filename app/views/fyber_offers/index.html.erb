<p id="notice"><%= notice %></p>

<h1>Listing Fyber Offers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fyber_offers.each do |fyber_offer| %>
      <tr>
        <td><%= link_to 'Show', fyber_offer %></td>
        <td><%= link_to 'Edit', edit_fyber_offer_path(fyber_offer) %></td>
        <td><%= link_to 'Destroy', fyber_offer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fyber offer', new_fyber_offer_path %>
